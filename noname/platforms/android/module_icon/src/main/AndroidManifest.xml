<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="online.nonamekill.android.module.icon">

    <application>

        <activity-alias
            android:name="online.nonamekill.android.module.icon.zhangba"
            android:enabled="false"
            android:exported="true"
            android:icon="@drawable/icon_noname_zhangba"
            android:label="@string/activity_name"
            android:targetActivity="online.nonamekill.android.MainActivity">

            <intent-filter android:label="@string/launcher_name">
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.DEFAULT" />
                <!-- zip -->
                <data
                    android:host="*"
                    android:mimeType="application/zip"
                    android:scheme="content" />
                <!-- 7z -->
                <data
                    android:host="*"
                    android:mimeType="application/7z"
                    android:scheme="content" />
                <!-- rar -->
                <data
                    android:host="*"
                    android:mimeType="application/rar"
                    android:scheme="content" />
                <!-- gz -->
                <data
                    android:host="*"
                    android:mimeType="application/x-gzip"
                    android:scheme="content" />
                <!-- tar -->
                <data
                    android:host="*"
                    android:mimeType="application/x-tar"
                    android:scheme="content" />
                <!-- cpio -->
                <data
                    android:host="*"
                    android:mimeType="application/x-cpio"
                    android:scheme="content" />
            </intent-filter>
        </activity-alias>

